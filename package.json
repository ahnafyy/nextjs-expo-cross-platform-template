{
  "name": "nextjs-expo-cross-platform-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "expo:start": "expo start",
    "expo:reset-project": "node ./scripts/reset-project.js",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . && next lint"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/",
      "<rootDir>/.expo/",
      "<rootDir>/e2e/",
      "<rootDir>/__tests__/utils/"
    ],
    "collectCoverageFrom": [
      "hooks/**/*.{ts,tsx}",
      "vendors/daisyui/Button/**/*.{ts,tsx}",
      "vendors/daisyui/Menu/**/*.{ts,tsx}",
      "vendors/daisyui/Drawer/**/*.{ts,tsx}",
      "app/**/*.{ts,tsx}",
      "!**/*.d.ts",
      "!**/node_modules/**",
      "!**/__tests__/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "dependencies": {
    "@expo/next-adapter": "^6.0.0",
    "@react-native-community/slider": "^4.5.5",
    "@react-native-picker/picker": "~2.9.0",
    "expo": "^52.0.35",
    "expo-auth-session": "~6.0.3",
    "expo-constants": "^17.0.6",
    "expo-document-picker": "~13.0.3",
    "expo-font": "^13.0.3",
    "expo-linking": "^7.0.5",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "^2.0.1",
    "firebase": "^11.3.1",
    "next": "^14.2.24",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "^0.76.7",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "~4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "~15.8.0",
    "react-native-web": "^0.19.13",
    "react-native-webview": "~13.12.5",
    "twrnc": "^4.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@playwright/test": "^1.49.0",
    "@react-navigation/native": "^7.0.14",
    "@tailwindcss/cli": "^4.0.6",
    "@tailwindcss/postcss": "^4.0.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.8.1",
    "@types/jest": "^29.5.14",
    "@types/react": "19.1.2",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^19.0.0",
    "babel-preset-expo": "^12.0.8",
    "daisyui": "^4.12.23",
    "eslint": "^8.57.1",
    "eslint-config-expo": "^7.1.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expo": "^52.0.4",
    "nativewind": "^4.1.23",
    "postcss": "^8.5.2",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "~3",
    "typescript": "^5.7.3"
  },
  "private": true,
  "volta": {
    "node": "22.14.0",
    "yarn": "4.9.1"
  },
  "packageManager": "yarn@4.9.1"
}
